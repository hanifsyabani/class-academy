generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Teacher {
  id Int @id @default(autoincrement())
  fullName String?
  email String?
  class String?
  gender String?
  password String?
  phone String?
  subject String?
  Designation String?
  image String?
  about String?
  age String?
  role String?
}

model Student {
   id Int @id @default(autoincrement())
  fullName String
  email String
  class String
  gender String
  nik String
  password String
  phone String
  Designation String?
}
